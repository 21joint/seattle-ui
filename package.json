    "style-ext-html-webpack-plugin": "^3.4.7",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.11.1",
    "webpack-cli": "^3.0.3",
    "webpack-dashboard": "^2.0.0",
